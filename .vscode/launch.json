{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug WebApp",
      "type": "node-terminal",
      "request": "launch",
      "cwd": "${workspaceFolder}/web-client",
      "command": "npm run dev",
      "serverReadyAction": {
        "pattern": "started server on .+, url: (https?://.+)",
        "uriFormat": "%s",
        "action": "debugWithChrome"
      }
    },
    {
      "name": "sutwa-account-service",
      "type": "node",
      "request": "launch",
      "cwd": "${workspaceFolder}/Sutwa.Microservices/sutwa-account-service",
      "runtimeExecutable": "npm",
      "runtimeArgs": [
        "run-script",
        "debug"
      ]
    }
  ],
  "compounds": [
    {
      "name": "Debug All",
      "configurations": ["sutwa-account-service", "Debug WebApp"]
    }
  ]
}