{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Client Web App",
      "request": "launch",
      "type": "pwa-chrome",
      "url": "http://localhost:3000",
      "webRoot": "${workspaceFolder}/src",
      "skipFiles": [
        "node_modules/**/*.js"
      ]
    },
    {
      "name": "sutwa-account-service",
      "type": "node",
      "request": "launch",
      "cwd": "${workspaceFolder}/Sutwa.Microservices/sutwa-account-service",
      "runtimeExecutable": "npm",
      "runtimeArgs": [
        "run-script",
        "debug"
      ]
    }
  ],
  "compounds": [
    {
      "name": "Debug All",
      "configurations": ["sutwa-account-service", "Client Web App"]
    }
  ]
}